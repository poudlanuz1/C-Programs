//WAP to give the roots of quadratic equation in C.
#include <stdio.h>
#include <math.h>
int main(){
	int a,b,c;
	float root1,root2;
	printf("Enter the value of a,b and c:");
	scanf("%d%d%d",&a,&b,&c);
	root1= -b+(pow(pow(b,2)-4*a*c,0.5))/(2*a);
	root2= -b-(pow(pow(b,2)-4*a*c,0.5))/(2*a);
	printf("The first root is:%.2f\n",root1);
	printf("The second root is:%.2f\n",root2);
	return 0;	
}
